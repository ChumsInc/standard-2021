#CartDrawer .offcanvas-header {
    border-bottom: 1px solid var(--bs-gray-200);
}

#CartDrawer .offcanvas-body {
    height: calc(100vh - 5rem);
}

#CartDrawer a:not(.btn) {
    color: var(--chums-drawer-color);
}

#CartContainer {
    height: 100%;
}

#CartContainer form {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    height: 100%;
}

#CartContainer form .cart-items {
    flex: 1 1 auto;
    overflow: auto;
}

#CartContainer .ajaxcart__product img {
    max-width: 100%;
    height: auto;
}

#CartContainer .ajaxcart__product-name--wrapper {
    margin-bottom: 0.75rem;
}

#CartContainer .ajaxcart__product-name {
    display: block;
}

#CartContainer .ajaxcart__product-meta {
    font-size: 0.85rem;
}

#CartContainer form .cart-footer {
    flex: 0 0 auto;
}

#CartContainer button[type="submit"] {
    display: block;
    text-transform: uppercase;
    color: var(--chums-drawer-button-color);
    background-color: var(--chums-drawer-button-bg-color);
}

#CartContainer button[type="submit"]:hover {
    color: var(--chums-drawer-button-color);
    background-color: var(--chums-drawer-button-hover-bg-color);
}

.variant-input-wrap--colors .variant-input--sold-out {
    opacity: var(--chums-swatch-sold-out-opacity, 0.5);
}

.product--zoom-modal .modal-title {
    color: var(--chums-body-color);
}

.product-carousel-thumbs {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: center;
    max-height: calc((var(--chums-thumb-height) + (2 * var(--chums-thumb-rows))) * 2.5);
    overflow-y: auto;
}

@media (min-width: var(--media-width-lg)) {
    .product-carousel-thumbs {
        max-height: unset;
    }
}

.product-carousel-thumbs.product-carousel-thumbs--hidden {
    display: none;
}

.product-carousel-thumbs .product-carousel-thumb {
    display: block;
    padding: var(--chums-thumb-padding);
    position: relative;
}

.product-carousel-thumbs .product-carousel-thumb .product-carousel-button {
    padding: 0;
}

.product-carousel-thumbs .product-carousel-thumb .img-thumbnail {
    border-radius: 0;
}

.product-carousel-thumbs .product-carousel-thumb.thumb--current-sku {
    display: block;
    order: 1;
}

.product-carousel-thumbs .product-carousel-thumb.thumb--all-skus {
    display: block;
    order: 2;
}

.product-carousel-thumbs .product-carousel-thumb.thumb--active .img-thumbnail {
    border-color: black;
}

.product-section .row .product-images-right {
    order: 2;
}

.product-carousel-thumbs .product-carousel-thumb--play-icon {
    position: absolute;
    top: 0;
    right: 0;
    background-color: var(--chums-body-bg-color, white);
    margin: calc(0.25rem + 3px);
    z-index: 1;
    font-size: 0;
    width: calc(33.333% - 6px);
    height: auto;
}

.product-carousel-thumbs .product-carousel-thumb--play-icon .icon {
    display: inline-block;
    width: 100%;
    height: 100%;
    vertical-align: middle;
    fill: rgba(var(--chums-body-color-rgb), 0.5);
    border: 1px solid rgba(var(--chums-body-color-rgb), 0.1);
}

@media (min-width: 768px) {

    .product-container {
        display: grid;
        grid: auto / 1fr 1fr;
        gap: 0.5rem 3rem;
    }

    .product-container .product--images {
        grid-column: 1;
        grid-row: 1 / 5;
    }

}

.product-single__title {
    margin-bottom: 5px;
    word-wrap: break-word;
}

.product-single__sku {
    margin-bottom: 0.333rem;
}

.product-single__form {
    margin-bottom: 2rem;
}

.product-single__variants {
    display: none;
}

.product__price {
    color: var(--chums-body-color);
    margin-right: 0.333rem;
}

.product-price--compare {
    padding-right: 0.333rem;
    display: inline-block;
    text-decoration: line-through;
}

.product__price-savings {
    color: var(--chums-savings-color);
    white-space: nowrap;
}

.product__quantity {
    margin-bottom: 1rem;
}


@media only screen and (min-width: 769px) {
    .product-single__title {
        margin-bottom: 0.5rem;
    }

    .product__quantity {
        margin-bottom: 2rem;
    }

    .product__quantity input {
        max-width: 5rem;
    }
}

.add-to-cart {
    border-color: #ce0e2d;
    color: #ce0e2d;
}

.shopify-payment-button {
    margin-top: 0.5rem;
}

.payment-buttons button {
    text-transform: uppercase;
    letter-spacing: 0.2em;
}

.payment-buttons .add-to-cart,
.payment-buttons .shopify-payment-button,
.payment-buttons .shopify-payment-button__button--unbranded {
    min-height: 50px;
}

.payment-buttons .shopify-payment-button__button--unbranded {
    background-color: var(--chums-red);
    transition-delay: .25s;
}

.variant-wrapper {
    margin-bottom: -12px;
}

.variant-wrapper--dropdown {
    display: inline-block;
    max-width: 100%;
    margin-right: calc(var(--chums-gutter) / 2);
    margin-bottom: 0;
}

.variant__label[for] {
    display: block;
    margin-bottom: 10px;
    cursor: default;
    font-weight: bold;
}

.variant__label-info {
    text-transform: none;
    font-weight: normal;
    margin-left: 0.5rem;
}

.variant-input-wrap {
    border: 0;
    padding: 0;
    margin: 0 0 calc(var(--chums-gutter) / 1.5);
    position: relative;
}

.variant-input-wrap input {
    clip: rect(0 0 0 0);
    clip: rect(0, 0, 0, 0);
    overflow: hidden;
    position: absolute;
    height: 1px;
    width: 1px;
}

.variant-input-wrap label {
    position: relative;
    display: inline-block;
    font-weight: normal;
    padding: 7px 15px 7px;
    margin: 0 8px 12px 0;
    background-color: var(--chums-body-bg-color);
    box-shadow: 0 0 0 1px var(--chums-border-color);
}

.variant-input-wrap label.disabled {
    color: var(--chums-border-color);
}

.variant-input-wrap label.disabled:after {
    position: absolute;
    content: "";
    left: 50%;
    top: 0;
    bottom: 0;
    border-left: 1px solid;
    border-color: var(--chums-border-color);
    transform: rotate(45deg);
}

.variant-input-wrap input[type='radio']:focus + label {
    box-shadow: 0 0 0 1px var(--chums-body-color);
}

.variant-input-wrap input[type='radio']:checked + label {
    box-shadow: 0 0 0 2px var(--chums-body-color);
}

.variant-input-wrap.variant-input-wrap--colors .variant-input--disabled:not(.variant-input--sold-out) {
    display: none;
}

.variant-input {
    display: inline-block;
}

label.color-swatch, .tag--swatch, .color-swatch {
    position: relative;
    display: block;
    text-indent: -9999px;
    overflow: hidden;
    margin: 0 4px 4px;
    background-position: center;
    background-size: cover;
    background-repeat: no-repeat;
    width: 2.5rem;
    height: 2.5rem;
    padding: 0;
    box-shadow: 0 0 0 1px #e8e8e1;
    transition: box-shadow .2s ease;
    background-color: #ce0e2d;
}

label.color-swatch:before,
.tag--swatch:before,
.color-swatch:before {
    border: 3px solid white;
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 2;
}

@media (max-width: 767.98px) {

    .product-carousel-thumbs {
        max-height: unset;
        overflow-y: unset;
        height: fit-content;
    }

    .product-carousel-thumbs .product-carousel-thumb img {
        display: none;
    }

    .product-carousel-thumbs .product-carousel-thumb.thumb--image .product-carousel-button {
        background-color: rgba(var(--chums-body-color-rgb), 0.5);
        width: 0.75rem;
        height: 0.75rem;
        margin: 0.5rem;
        border-radius: 100%;
    }

    .product-carousel-thumbs .product-carousel-thumb.thumb--image.thumb--active .product-carousel-button {
        background-color: rgba(var(--chums-red-rgb), 1);
    }

    .product-carousel-thumbs .product-carousel-thumb.thumb--external_video .product-carousel-button {
        width: 0.75rem;
        height: 0.75rem;
        margin: 0.25rem;
    }

    .product-carousel-thumbs .product-carousel-thumb.thumb--external_video .product-carousel-button {
        background-image: url("data:image/svg+xml,%3Csvg width='26' height='26' viewBox='0 0 26 26' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath fill-rule='evenodd' clip-rule='evenodd' d='M1 25H25V1H1V25Z' fill='white'/%3E%3Cpath d='M0.5 25V25.5H1H25H25.5V25V1V0.5H25H1H0.5V1V25Z' stroke='black' stroke-opacity='0.05'/%3E%3Cpath fill-rule='evenodd' clip-rule='evenodd' d='M9.71814 6.71984C9.0517 6.31605 8.19995 6.79588 8.19995 7.5751V18.311C8.19995 19.1138 9.09826 19.5893 9.76217 19.1379L18.1123 13.4612C18.7146 13.0518 18.6912 12.1564 18.0682 11.779L9.71814 6.71984Z' fill='%233A3A3A' fill-opacity='0.6'/%3E%3C/svg%3E%0A");
        background-repeat: no-repeat;
        background-position: center center;
    }

    .product-carousel-thumbs .product-carousel-thumb.thumb--external_video .product-carousel-button .product-carousel-thumb--play-icon svg {
        display: none;
    }
}

.collection-filter {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    margin-bottom: 2rem;
}


.collection-filter__item--count {
    flex: 1 1 170%;
    text-align: center;
}

.collection-filter__item--sort {
    flex: 0 1 50%;
    padding-left: 0.5rem;
}

.collection-filter__item--warning {
    flex: 0 1 50%;
    padding: 5px 10px;
    background-color: #ce0e2d;
    color: #fff;
    font-weight: 700;
    text-align: center;
}

.collection-filter__item--warning.hidden {
    display: none;
}

.collection-sidebar ul.tag-list {
    list-style: none outside;
    margin: 0;
    padding: 0;
}

.collection-sidebar ul.tag-list a {
    display: block;
    margin-top: 1rem;
    margin-bottom: 1rem;
}

.tag-list--checkbox .tag-list--item {
    display: block;
    min-height: 1.5rem;
    padding-left: 1.5em;
    margin-bottom: 0.125rem;
}

.tag-list--checkbox .tag-list--item input {
    width: 1em;
    height: 1em;
    margin-top: .25em;
    vertical-align: top;
    background-color: #fff;
    background-repeat: no-repeat;
    background-position: center;
    background-size: contain;
    border: 1px solid rgba(0, 0, 0, .25);
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    -webkit-print-color-adjust: exact;
    color-adjust: exact;
    float: left;
    margin-left: -1.5em;
}

.tag-list--checkbox .tag-list--item label small {
    white-space: nowrap;
}

.tag-list--checkboxes a:before {
    border: 1px solid #e8e8e1;
    height: 16px;
    width: 16px;
}

.tag-list--checkboxes a:before {
    content: "";
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
}

.tag--remove {
    position: relative;
}

.tag--remove a {
    text-align: right;
}

.tag--remove .icon {
    position: absolute;
    right: 10px;
    top: 50%;
    transform: translateY(-50%);
    pointer-events: none;
    color: var(--chums-body-color);
}

.tag--inverse .icon {
    color: var(--chums-red);
}

.tag--inverse .icon:hover {
    color: white;
}

.tag--remove-filters {
    display: none;
}

.tag--remove-filters.has-filters {
    display: list-item;
}

.collection-sidebar__group .tag-list__header {
    display: block;
    text-transform: uppercase;
    letter-spacing: 0.2rem;
    width: 100%;
    padding: 1rem 0;
    position: relative;
    cursor: pointer;
}

.collection-sidebar__group .tag-list__header.collapsed .collapsible-trigger__icon > .icon-chevron-down {
    transform: scaleY(-1);
}

.collection-sidebar ul.tag-list--checkbox li {
    margin-top: 0.5rem;
    margin-bottom: 0.5rem;
