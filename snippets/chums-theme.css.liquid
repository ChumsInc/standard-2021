:root {
    /**
    global settings
     */
    --media-width-sm: 576px;
    --media-width-md: 768px;
    --media-width-lg: 992px;
    --media-width-xl: 1200px;
    --media-width-xxl: 1400px;

    --chums-z-idnex-nav-dropdowns: 5;
    --chums-z-index-drawer: 30;
    --chums-z-index-drawer-overlay: 26;
    --chums-z-index-modal: 1055;
    --chums-z-index-skip-to-content: 10000;
    --chums-z-index-sticky-header: 6;
    --chums-z-index-stuck-header: 20;
    --chums-z-index-header-search: 20;
    --chums-z-index-loader: 4;

    --chums-red: {{ settings.color_button | default: '#ce0e2d' }};
    --chums-red-rgb: 206, 14, 45;
    --chums-body-bg-color: {{ settings.color_body_bg | default: '#fff' }};
    --chums-body-color: {{ settings.color_body_text | default: '#1c1d1d' }};
    --chums-body-color-rgb: 28, 29, 29;
    --chums-font-family: 'Roboto';
    --chums-savings-color: {{ settings.color_savings_text | default: '#1c1d1d' }};
    --chums-border-color: {{ settings.color_borders | default: '#1c1d1d' }};

    --chums-announcement-bar-bg-color: {{ settings.color_announcement | default: '#1c1d1d' }};
    --chums-announcement-bar-color: {{ settings.color_announcement_text | default: '#fff' }};

    --chums-gutter: 30px;

    /* drawer settings */
    --chums-drawer-z-index: 30;
    --chums-drawer-z-index-overlay: 26;
    --chums-drawer-gutter-large: var(--chums-gutter);
    --chums-drawer-gutter-small: calc(var(--chums-gutter) / 2);
    --chums-drawer-color: {{ settings.color_drawer_text | default: '#fff' }};
    --chums-drawer-bg-color: {{ settings.color_drawer_background | default: '#1c1d1d' }};
    --chums-drawer-border-color: {{ settings.color_drawer_border | default: '#343535' }};
    --chums-drawer-button-color: {{ settings.color_button_text | default: '#fff' }};
    --chums-drawer-button-hover-bg-color: var(--chums-red);
    --chums-drawer-button-bg-color: {{ settings.color_drawer_button | default: '#a26b25' }};
    --chums-drawer-nav-width: 300px;
    --chums-drawer-cart-width: 300px;
    --chums-drawer-cart-width-lg: 400px;
    --chums-drawer-header-height: 70px;
    --chums-drawer-header-height-lg: 119px;
    --chums-drawer-footer-height: 130px;

    --chums-index-section-margin-small: calc(var(--chums-gutter) * 0.85);
    --chums-index-section-margin-large: calc(var(--chums-gutter) * 2.5);

    /* image overlays */
    --chums-image-overlay-color: {{ settings.color_image_overlay | default: '#000' }};
    --chums-image-overlay-opacity: {{ settings.color_image_overlay_opacity | divided_by: 100.0 }};
    --chums-image-overlay-text-shadow: {{ settings.color_image_overlay_text_shadow | divided_by: 100.0 }};
    --chums-image-small-bg-color: {{ settings.color_small_image_bg | default: '#eee' }};
    --chums-image-large-bg-color: {{ settings.color_large_image_bg | default: '#1c1d1d' }};
    --chums-grid-overlay-color: {{ settings.collection_grid_tint | default: '#000' }};
    --chums-grid-overlay-opacity: {{ settings.collection_grid_opacity | divided_by: 100.0 }};
    --chums-grid-col-gap: 1rem;
    --chums-grid-row-gap: 1rem;
    --chums-grid-cols--blog: 3;
    --chums-hero-text-color: #fff;

    /* collection settings */
    --chums-collection-hero-large: 420px;
    --chums-collection-hero-small: 250px;

    /**
    Product settings
     */
    --chums-swatch-sold-out-opacity: 0.5;
    --chums-thumb-height: 96px;
    --chums-thumb-rows: 2.5;
    --chums-thumb-padding: 0.25rem;

}

html,
body {
    background-color: var(--chums-body-bg-color);
    color: var(--chums-body-color);
}

body {
    font-weight: 300;
    font-family: var(--chums-font-family), var(--bs-body-font-family);
}

a {
    color: var(--chums-body-color);
    text-decoration: none;
}

h1, .h1, h2, .h2, h3, .h3 {
    font-family: Roboto, sans-serif;
    font-weight: 500;
    letter-spacing: 0;
    line-height: 1;
    text-transform: uppercase;
}

h4,
.h4 {
    text-transform: uppercase;
    letter-spacing: .2em;
    font-size: .8rem;
}

b, strong {
    font-weight: 700;
}

button {
    background: none;
    border: none;
    display: inline-block;
    cursor: pointer;
}

label:not(.variant__button-label),
.label {
    text-transform: uppercase;
    letter-spacing: .2em;
    font-size: .8em;
}

.hide {
    display: none !important;
}

.btn {
    text-transform: uppercase;
    font-weight: 700;
    letter-spacing: 0.2em;
    border-radius: 0;
}

.btn--full {
    width: 100%;
    transition: none;
    padding: 0.875rem 1.25rem;
}

.btn--secondary, .rte .btn--secondary {
    color: #000;
    border: 1px solid var(--bs-gray-300);
    background-color: transparent;
}

.btn--secondary.btn--small {
    font-weight: normal;
}

.btn--large {
    padding: 1rem 1.25rem;
}

.btn--red {
    border-color: var(--chums-red);
    background-color: var(--chums-red) !important;
    color: white;
}

.btn--inverse {
    background-color: transparent;
    color: #fff;
    border: 2px solid #fff;
}

.btn--inverse:hover,
.btn--inverse:focus {
    background-color: transparent;
}

.hero__link .btn--inverse {
    color: white;
    border-color: white;
}

.btn--inverse--red {
    border-color: var(--chums-red);
    color: var(--chums-red) !important;
}

.btn--inverse--red:hover {
    background-color: var(--chums-red);
    color: white !important;
}

