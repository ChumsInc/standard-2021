.site-header {
    position: relative;
    padding: 7px 0;
    background: var(--chums-body-bg-color);
}

.site-header .screen-width {
    padding: 0 50px;
}

@media (min-width: 768px) {
    .site-header {
        padding: 20px 0;
    }
}

.header-sticky-wrapper {
    position: relative;
}

.header-wrapper--sticky {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    z-index: var(--chums-z-index-sticky-header);
