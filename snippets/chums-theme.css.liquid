@media screen and (min-width: 769px) {
    .collection-hero {
        height: var(--chums-collection-hero-large);
    }

    .collection-hero__content {
        padding: var(--chums-gutter) 0;
    }

    .overlaid-header .collection-hero__content {
        padding: var(--chums-gutter) 0;
    }
}

.section-header--hero {
    position: relative;
    flex: 1 1 100%;
    color: var(--chums-hero-text-color);
    margin-bottom: 0;
}

.section-header--hero a {
    color: var(--chums-hero-text-color);
}

.section-header--hero .breadcrumb {
    justify-content: center;
}

.section-header--hero .section-header__title {
    position: relative;
}

.section-header__shadow {
    position: relative;
    display: inline-block;
}

.section-header__shadow:before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: radial-gradient(rgba(0, 0, 0, var(--chums-image-overlay-text-shadow)) 0%, rgba(0, 0, 0, 0) 60%);
    margin: -100px -200px -100px -200px;
}

.section-header__shadow .breadcrumb,
.section-header__shadow .section-header__title {
    position: relative;
}

.section-header__title {
    margin-bottom: 0;
}

.section-header--with-link .section-header__title {
    flex: 1 1 auto;
}

@media screen and (min-width: 769px) {
    .section-header--hero .section-header__title {
        font-size: 3.5rem;
    }
}

.section-header__link {
    flex: 0 1 auto;
    margin-top: calc(var(--chums-gutter) / 2);
}

.section-header--with-link .section-header__link {
    margin-top: 0;
}

.jdgm-rev-widg__reviews {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.jdgm-rev-widg__reviews .jdgm-rev {
    flex: 0 0 50%;
}

.jdgm-rev-widg__reviews .jdgm-rev .jdgm-rev__content {
    padding-left: 2rem;
}

@media only screen and (max-width: 768px) {
    .jdgm-rev-widg__reviews .jdgm-rev {
        flex-basis: 100%;
    }
}

.jdgm-preview-badge .jdgm-star {
    color: #808080 !important;
}

#Slideshow-536ec014-0f96-457f-83d3-96c846f1e6ee .slick-slide {
    z-index: 4 !important;
}

[data-aos=hero__animation].loaded.aos-animate .hero__media, [data-aos=hero__animation].loaded.aos-animate .hero__image.lazyloaded, [data-aos=hero__animation].loaded.aos-animate .hero__image--svg, .hero.loaded.aos-animate .slideshow__slide.slick-active .hero__media, .hero.loaded.aos-animate .slideshow__slide.slick-active .hero__image.lazyloaded, .hero.loaded.aos-animate .slideshow__slide.slick-active .hero__image--svg {
